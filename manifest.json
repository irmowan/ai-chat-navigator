{
  "manifest_version": 3,
  "name": "ChatGPT Turn Navigator",
  "version": "1.0.0",
  "description": "Quickly navigate and locate user queries in ChatGPT conversations",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*", "https://chat.openai.com/*"],
      "js": ["content.js"],
      "css": ["sidebar.css"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
