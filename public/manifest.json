{
  "manifest_version": 3,
  "name": "AI Turn Navigator",
  "version": "1.0.0",
  "description": "Navigate AI conversations with a floating sidebar for ChatGPT, Gemini, and more",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*",
        "https://chat.openai.com/*",
        "https://gemini.google.com/*",
        "https://copilot.microsoft.com/*"
      ],
      "js": ["src/content/index.ts"]
    }
  ]
}
